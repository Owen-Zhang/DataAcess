<CommandFile>
  <CommandList>
    <Command Name="GetCustomerByCustomerID" DataBase="Northwind1" CommandType="Text">
      <CommandText>
        <![CDATA[
        select * 
        from dbo.Customers with(nolock)
        where CustomerID = @ID
      ]]>
      </CommandText>
      <Parameters>
        <Parameter Name="@ID" DbType="StringFixedLength" Direction="Input"/>
      </Parameters>
    </Command>
